{
"name": "56微统计",
"manifest_version": 2,
"version": "1.0",
"description": "一个通过56员工考勤记录生成统计图表的扩展",
"icons": { "128": "icon.jpg" },
"content_scripts": [{
"matches": ["http://*/*"],
"js": ["js/content_script.js"]
}],
"page_action":{
    "default_icon":"icon.png",
    "default_title":"56微统计",
    "default_popup":"html/popup.html"
},
"background": {
    "scripts": ["js/background.js"]
  },
"permissions":[
    "tabs"
]
}